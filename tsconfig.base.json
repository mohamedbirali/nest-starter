{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noPropertyAccessFromIndexSignature": false,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": ["node", "Multer"],
    "baseUrl": ".",
    "paths": {
      "@bee/common/api-key": ["libs/bee/common/api-key/src/index.ts"],
      "@bee/common/auth": ["libs/bee/common/auth/src/index.ts"],
      "@bee/common/aws": ["libs/bee/common/aws/src/index.ts"],
      "@bee/common/core": ["libs/bee/common/core/src/index.ts"],
      "@bee/common/dashboard": ["libs/bee/common/dashboard/src/index.ts"],
      "@bee/common/database": ["libs/bee/common/database/src/index.ts"],
      "@bee/common/debugger": ["libs/bee/common/debugger/src/index.ts"],
      "@bee/common/error": ["libs/bee/common/error/src/index.ts"],
      "@bee/common/file": ["libs/bee/common/file/src/index.ts"],
      "@bee/common/helper": ["libs/bee/common/helper/src/index.ts"],
      "@bee/common/logger": ["libs/bee/common/logger/src/index.ts"],
      "@bee/common/mailer": ["libs/bee/common/mailer/src/index.ts"],
      "@bee/common/message": ["libs/bee/common/message/src/index.ts"],
      "@bee/common/mongodb": ["libs/bee/common/mongodb/src/index.ts"],
      "@bee/common/pagination": ["libs/bee/common/pagination/src/index.ts"],
      "@bee/common/policy": ["libs/bee/common/policy/src/index.ts"],
      "@bee/common/prisma": ["libs/bee/common/prisma/src/index.ts"],
      "@bee/common/request": ["libs/bee/common/request/src/index.ts"],
      "@bee/common/response": ["libs/bee/common/response/src/index.ts"],
      "@bee/common/types": ["libs/bee/common/types/src/index.ts"],
      "@bee/configs": ["libs/bee/configs/src/index.ts"],
      "@bee/entery": ["libs/bee/entery/src/index.ts"],
      "@bee/env": ["libs/bee/env/src/index.ts"],
      "@bee/health": ["libs/bee/health/src/index.ts"],
      "@bee/jobs": ["libs/bee/jobs/src/index.ts"],
      "@bee/languages": ["libs/bee/languages/src/index.ts"],
      "@bee/modules/chat": ["libs/bee/modules/chat/src/index.ts"],
      "@bee/modules/permission": ["libs/bee/modules/permission/src/index.ts"],
      "@bee/modules/role": ["libs/bee/modules/role/src/index.ts"],
      "@bee/modules/user": ["libs/bee/modules/user/src/index.ts"],
      "@bee/router": ["libs/bee/router/src/index.ts"],
      "crypto": ["node_modules/crypto-js"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
